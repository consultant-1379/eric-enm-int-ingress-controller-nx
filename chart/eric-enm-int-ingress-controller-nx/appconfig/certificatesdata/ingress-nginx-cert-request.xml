<applications xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="CertificateRequest.xsd">
    <application>
        <certificates>
            <certificate>
                <certificatechain>true</certificatechain>
                <tbscertificate>
                    <subject>
                        <entityname>##hostname##</entityname>
                        <distinguishname>CN=##UI_PRES_SERVER##</distinguishname>
                    </subject>
                    <certificateextension>
                        <subjectalternativename>
                            <dns>##UI_PRES_SERVER##</dns>
                            <ipaddress>##LOADBALANCER_IPv4##</ipaddress>
                            <ipaddress>##LOADBALANCER_IPv6##</ipaddress>
                        </subjectalternativename>
                    </certificateextension>
                </tbscertificate>
                <endentityprofilename>ENM_System_UI_SAN_LB_EP</endentityprofilename>
                <keypair>
                    <keypairsize>2048</keypairsize>
                    <keypairalgorithm>RSA</keypairalgorithm>
                </keypair>
                <keystore>
                    <base64keystore>
                        <storealias>ingress-nginx-enm-ingress</storealias>
                        <keyfilelocation>/ericsson/ingress/ingress-nginx-enm_ingress.key</keyfilelocation>
                        <certificatefilelocation>/ericsson/ingress/ingress-nginx-enm_ingress.crt</certificatefilelocation>
                    </base64keystore>
                </keystore>
            </certificate>
        </certificates>
        <crlupdatemode>CrlNotManaged</crlupdatemode>
    </application>
    <user>
        <username>root</username>
        <usergroup>root</usergroup>
    </user>
</applications>